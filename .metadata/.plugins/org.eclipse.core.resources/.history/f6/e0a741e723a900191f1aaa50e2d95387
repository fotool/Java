package chapter5;

public class Student{
	int id;
	String name;
	int grade;
	String address;
	
	//original constructor
	public Student() {
		System.out.println("기본 생성자 호출!");
	}
	// 이름을 패러미터로 가지는 생성자.
	public Student(String p_name) {
		name = p_name;
		System.out.println("이름을 가진 생성자 호출!");
	}
	public void study() {
		System.out.println(name + "이 공부한다.");
	}
}// end of class Student