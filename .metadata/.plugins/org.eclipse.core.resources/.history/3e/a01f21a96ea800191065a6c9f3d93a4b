package practice;

public class practice1 {

	public static void main(String[] args) {
		int num1 = 10; 
		int num2 = 2;
		char operator;
		
		if (char = '+') {
			System.out.println(num1 + operator + num2);
			
		}else if (char = '-') {
			System.out.println(num1 + operator + num2);
			
		}else if (char = '*') {
			System.out.println(num1 + operator + num2);
		}else if (char = '/'){
			System.out.println(num1 + operator + num2);
		}
		
	}

}
